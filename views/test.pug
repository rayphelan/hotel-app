    .wrapper
      // Sidebar Holder
      nav#sidebar
        .sidebar-header
          h3 Bootstrap Sidebar
          strong BS
        ul.list-unstyled.components
          li.active
            a(href="#homeSubmenu" data-toggle="collapse" aria-expanded="false")
              i.glyphicon.glyphicon-home
              |                             Home
            ul#homeSubmenu.collapse.list-unstyled
              li
                a(href="#") Home 1
              li
                a(href="#") Home 2
              li
                a(href="#") Home 3
          li
            a(href="#")
              i.glyphicon.glyphicon-briefcase
              |                             About
            a(href="#pageSubmenu" data-toggle="collapse" aria-expanded="false")
              i.glyphicon.glyphicon-duplicate
              |                             Pages
            ul#pageSubmenu.collapse.list-unstyled
              li
                a(href="#") Page 1
              li
                a(href="#") Page 2
              li
                a(href="#") Page 3
          li
            a(href="#")
              i.glyphicon.glyphicon-link
              |                             Portfolio
          li
            a(href="#")
              i.glyphicon.glyphicon-paperclip
              |                             FAQ
          li
            a(href="#")
              i.glyphicon.glyphicon-send
              |                             Contact
        ul.list-unstyled.CTAs
          li
            a.download(href="https://bootstrapious.com/tutorial/files/sidebar.zip") Download source
          li
            a.article(href="https://bootstrapious.com/p/bootstrap-sidebar") Back to article
      // Page Content Holder
      #content
        nav.navbar.navbar-default
          .container-fluid
            .navbar-header
              button#sidebarCollapse.btn.btn-info.navbar-btn(type="button")
                i.glyphicon.glyphicon-align-left
                span Toggle Sidebar
            #bs-example-navbar-collapse-1.collapse.navbar-collapse
              ul.nav.navbar-nav.navbar-right
                li
                  a(href="#") Page
                li
                  a(href="#") Page
                li
                  a(href="#") Page
                li
                  a(href="#") Page
